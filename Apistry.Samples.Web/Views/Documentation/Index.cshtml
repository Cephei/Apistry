            {
                "<span class="attribute">type</span>": <span class="value"><span class="string">"folder"</span></span>,
                "<span class="attribute">id</span>": <span class="value"><span class="string">"0"</span></span>,
                "<span class="attribute">sequence_id</span>": <span class="value"><span class="literal">null</span></span>,
                "<span class="attribute">etag</span>": <span class="value"><span class="literal">null</span></span>,
